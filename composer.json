{
    "name": "liquid-cats/laravel-ruler",
    "description": "Business Rules System For Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "I.Shabanov",
            "email": "ishabanov@liquid-cats.ru"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "laravel/framework": ">=5.5",
        "hoa/ruler": "2.17.05.16"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "LiquidCats\\Ruler\\": "src/LiquidCats/Ruler"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\LiquidCats\\Ruler\\": "tests/LiquidCats/Ruler"
        }
    },
    "scripts": {
        "fix": "@php vendor/bin/php-cs-fixer fix"
    }
}
